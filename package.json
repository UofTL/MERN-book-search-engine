    },
    "keywords": [
        "MERN",
        "Stack",
        "REACT",
        "Google",
        "Book",
        "Search",
        "API",
        "Full",
        "Stack"
    ],
    "author": "Eleonore Caclard",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/UofTL/MERN-book-search-engine/issues"
    },
    "dependencies": {
        "if-env": "^1.0.4"
    },
    "devDependencies": {
        "concurrently": "^5.1.0"
    },
    "homepage": "https://github.com/UofTL/MERN-book-search-engine#readme"
}
